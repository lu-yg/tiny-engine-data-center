      "type": "string",
      "required": true
    },
    "name_cn": {
      "type": "string"
    },
    "npm_name": {
      "type": "string"
    },
    "framework": {
      "type": "string"
    },
    "content": {
      "type": "json"
    },
    "assets": {
      "type": "json"
    },
    "screenshot": {
      "type": "text"
    },
    "createdBy": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "updatedBy": {
      "plugin": "users-permissions",
      "model": "user",
      "private": true
    },
    "last_build_info": {
      "type": "json"
    },
    "histories": {
      "collection": "block-history"
    },
    "groups": {
      "collection": "block-groups",
      "via": "blocks"
    },
    "categories": {
      "collection": "block-category",
      "via": "blocks"
    },
    "description": {
      "type": "text"
    },
    "tags": {
      "type": "json"
    },
    "author": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "current_history": {
      "model": "block-history"
    },
    "i18n_langs": {
      "collection": "i18n-lang",
      "via": "blocks"
    },
    "material_histories": {
