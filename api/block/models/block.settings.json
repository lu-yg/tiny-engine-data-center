      "collection": "material-histories",
      "via": "runtime_blocks"
    },
    "path": {
      "type": "string"
    },
    "occupier": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "isOfficial": {
      "type": "boolean",
      "default": false
    },
    "public": {
      "type": "integer",
      "default": 1,
      "required": false
    },
    "public_scope_tenants": {
      "collection": "tenant",
      "via": "blocks",
      "dominant": true
    },
    "isDefault": {
      "type": "boolean",
      "default": false
    },
    "tiny_reserved": {
      "type": "boolean",
      "default": false
    },
